{
    "name": "arus/api-authorization",
    "require": {
        "php": "^7.2",
        "arus/api-foundation": "^3.1",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "sunrise/http-factory": "^1.0",
        "zendframework/zend-permissions-rbac": "^3.0"
    },
    "require-dev": {
        "arus/php-coding-standard": "^1.0",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.11",
        "phpunit/phpunit": "^7.5",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.4",
        "sunrise/http-router": "^1.0",
        "vimeo/psalm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Arus\\Authorization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arus\\Authorization\\Tests\\": "tests"
        }
    },
    "repositories": {
        "arus/api-foundation": {
            "type": "vcs",
            "url": "git@gitlab.voshod.local:web/http-api-foundation.git"
        },
        "arus/php-coding-standard": {
            "type": "vcs",
            "url": "git@gitlab.voshod.local:web/php-coding-standard.git"
        }
    },
    "config": {
        "sort-packages": true
    }
}
